<Project Sdk="Microsoft.NET.Sdk">
  <!--https://github.com/microsoft/MSBuildSdks/blob/main/src/NoTargets/README.md-->

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
    <IsPackable>false</IsPackable>
    <IsTestProject>false</IsTestProject>
  </PropertyGroup>

  <Target Name="GetDocumentation">
    <Message Text="No documentation available." Importance="high" />
  </Target>
  
  <ItemGroup>
    <None Include="$(SolutionDir)docs\**\*.*" Pack="true" PackagePath="\docs" />
  </ItemGroup>

</Project>
