{
  "profiles": {
    "Nucleus.Dataloader.Cli": {
      "commandName": "Project",
      "commandLineArgs": "$(SolutionDir)\\Conf\\MongoDb"
    },
    "27018 (nucleus2) - Export": {
      "commandName": "Project",
      "commandLineArgs": "--SourcePath=$(SolutionDir)\\Conf\\MongoDb\\SampleData",
      "environmentVariables": {
        "MongoDatabase__ConnectionString": "mongodb://localhost:27018",
        "MongoDatabase__DatabaseName": "nucleus2"
      }
    },
    "27018 (nucleus) - Export": {
      "commandName": "Project",
      "commandLineArgs": "--SourcePath=$(SolutionDir)\\Conf\\MongoDb\\SampleData",
      "environmentVariables": {
        "MongoDatabase__ConnectionString": "mongodb://localhost:27018",
        "MongoDatabase__DatabaseName": "nucleus"
      }
    },
    "27018 (nucleus) - Import": {
      "commandName": "Project",
      "commandLineArgs": "--SourcePath=$(SolutionDir)\\Conf\\MongoDb\\SampleData --Action=Import",
      "environmentVariables": {
        "MongoDatabase__ConnectionString": "mongodb://localhost:27018",
        "MongoDatabase__DatabaseName": "nucleus"
      }
    }
  }
}