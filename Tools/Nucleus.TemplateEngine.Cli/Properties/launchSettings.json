{
  "profiles": {
    "Generate - Service Endpoints Markdown": {
      "commandName": "Project",
      "commandLineArgs": "--input $(SolutionDir)docs\\swagger.json\r\n--output $(SolutionDir)docs\\Service-Endpoints.md\r\n--Template Service-Endpoints\r\n--file-template-path $(ProjectDir)$(OutputPath) "
    },
    "Generate - XmlDocs to Markdown": {
      "commandName": "Project",
      "commandLineArgs": "--input $(SolutionDir)Nucleus.Api\\bin\\Debug\\net6.0\\*.xml\r\n--output $(SolutionDir)docs\\Libraries\\[file].md\r\n--Template Documentation.md\r\n--file-template-path $(ProjectDir)$(OutputPath)"
    },
    "Generate - Test Results to Markdown": {
      "commandName": "Project",
      "commandLineArgs": "--input $(SolutionDir)TestResults\\*.trx\r\n--output $(SolutionDir)docs\\Tests\\[file].md\r\n--Template TestResultsToMarkdown.md\r\n--file-template-path $(ProjectDir)$(OutputPath)\r\n--input-type XML"
    }
  }
}